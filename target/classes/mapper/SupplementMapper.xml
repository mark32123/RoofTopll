<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ll.rooftopll.mapper.SupplementLogMapper">
    <insert id="insertLog">
        INSERT INTO supplement_log (session_id, name, dosage, taken_time)
        VALUES (#{sessionId}, #{name}, #{dosage}, #{takenTime})
    </insert>

    <select id="selectBySessionId" resultType="com.ll.rooftopll.entity.SupplementLog">
        SELECT * FROM supplement_log WHERE session_id = #{sessionId}
    </select>
</mapper>