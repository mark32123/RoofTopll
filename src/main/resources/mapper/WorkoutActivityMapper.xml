<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ll.rooftopll.mapper.WorkoutActivityMapper">
    <insert id="insertActivity" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO workout_activity (session_id, exercise_id, order_num)
        VALUES (#{sessionId}, #{exerciseId}, #{orderNum})
    </insert>
</mapper>